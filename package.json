    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-p5": "^1.3.30"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-tailwindcss": "^3.5.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.2"
  }
